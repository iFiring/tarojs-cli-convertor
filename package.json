{
  "name": "tarojs-cli-convertor",
  "version": "4.1.10",
  "description": "cli tool for taro-convert",
  "author": "O2Team",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prod": "pnpm run build",
    "prebuild": "pnpm run clean",
    "build": "./node_modules/.bin/tsc",
    "dev": "./node_modules/.bin/tsc -w",
    "clean": "./node_modules/.bin/rimraf dist",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"{src, __tests__}/**/*.{ts,tsx,js,jsx}\" && eslint src  __tests__ --ext .js --ext .jsx --ext .ts --ext .tsx --fix",
    "test": "jest",
    "test:ci": "cross-env NODE_ENV=test jest --ci -i --coverage --silent",
    "test:clear": "jest --clearCache",
    "test:coverage": "jest --coverage",
    "updateSnapshot": "jest --updateSnapshot"
  },
  "files": [
    "bin",
    "dist",
    "report"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro.git"
  },
  "bin": {
    "taro-convert": "bin/taro-convert"
  },
  "keywords": [
    "taro",
    "weapp"
  ],
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@babel/generator": "~7.24.4",
    "@babel/template": "~7.24.0",
    "@babel/traverse": "~7.26.10",
    "@tarojs/binding": "4.1.6",
    "@babel/types": "~7.24.0",
    "@tarojs/cli": "4.1.6",
    "@tarojs/helper": "4.1.6",
    "@tarojs/shared": "4.1.6",
    "@tarojs/taroize": "4.1.6",
    "@tarojs/transformer-wx": "4.1.6",
    "postcss": "~8.4.38",
    "postcss-taro-unit-transform": "4.1.6",
    "prettier": "~2.8.8"
  },
  "devDependencies": {
    "@babel/parser": "~7.24.4",
    "@tarojs/taro": "4.1.6",
    "rimraf": "~5.0.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "babel-jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "eslint-plugin-jest": "^26.1.3",
    "jest-environment-jsdom": "^29.6.4",
    "jest-environment-node": "^29.7.0",
    "jest-light-runner": "^0.6.0",
    "jest-preset-stylelint": "^7.0.0",
    "ts-jest": "^29.1.2"
  }
}
